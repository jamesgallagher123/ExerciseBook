def returnAnything (input: Any): Any = {
    return input
}

val doubleVariable: Double = 1.5
val stringVariable: String = "This is a string"
val integerVariable: Int = 1
val booleanVariable: Boolean = true

returnAnything(doubleVariable)
returnAnything(stringVariable)
returnAnything(integerVariable)
returnAnything(booleanVariable)